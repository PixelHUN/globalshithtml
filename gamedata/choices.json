[ { "aText": "Veszek újat.", "bText": "Addig biciklizek", "minBudget": 5.0, "aOption": { "Temperature": 0.0, "Happyness": 2.0, "Wealthyness": 3.0, "CO": 3.0 }, "text": "Lerobbant az autód. Valószínűleg egy ideig eltarthat megjavítani.", "bOption": { "Temperature": 0.0, "Happyness": 1.0, "Wealthyness": 0.0, "CO": 0.0 } }, { "aText": "Felmondok.", "bText": "Nem mondok fel.", "minBudget": 5.0, "aOption": { "Temperature": 0.0, "Happyness": 0.0, "Wealthyness": 0.0, "CO": 0.0 }, "text": "A mai napod nagyon rossz volt. Úgy érzed nem fogod bírni tovább.", "bOption": { "Temperature": 0.0, "Happyness": 1.0, "Wealthyness": 2.0, "CO": 2.0 } }, { "aText": "Eh, bemegyek.", "bText": "Dehogy megyek.", "minBudget": 4.0, "aOption": { "Temperature": 0.0, "Happyness": -1.0, "Wealthyness": 1.0, "CO": 1.0 }, "text": "Nincs kedved ma dolgozni menni.", "bOption": { "Temperature": 0.0, "Happyness": 2.0, "Wealthyness": 0.0, "CO": 0.0 } }, { "aText": "Fejlesztem.", "bText": "Nincs rá nagy szükségem.", "minBudget": 7.0, "aOption": { "Temperature": 0.0, "Happyness": 2.0, "Wealthyness": 2.0, "CO": 2.0 }, "text": "Lehetőséged adódott fejleszteni az otthoni számítógépedet.", "bOption": { "Temperature": 0.0, "Happyness": 0.0, "Wealthyness": 0.0, "CO": 0.0 } }, { "aText": "Nem baj. Száradjanak ki.", "bText": "Megpróbálom rávenni őket.", "minBudget": 2.0, "aOption": { "Temperature": 0.0, "Happyness": 1.0, "Wealthyness": 3.0, "CO": 2.0 }, "text": "Barátaid nem isznak elég vizet.", "bOption": { "Temperature": 0.0, "Happyness": 1.0, "Wealthyness": 0.0, "CO": 0.0 } }, { "aText": "Kipróbálom.", "bText": "Elég fura. Inkább nem.", "minBudget": 5.0, "aOption": { "Temperature": 0.0, "Happyness": 0.0, "Wealthyness": 2.0, "CO": 1.0 }, "text": "Az interneten azt olvastad, hogy ha a kávédba étolajat teszel az egészségesebb számodra.", "bOption": { "Temperature": 0.0, "Happyness": 1.0, "Wealthyness": 0.0, "CO": 0.0 } }, { "aText": "Nem próbálom ki.", "bText": "Én is kipróbálom.", "minBudget": 2.0, "aOption": { "Temperature": 0.0, "Happyness": -1.0, "Wealthyness": 0.0, "CO": 0.0 }, "text": "A baráti társaságodban elterjedt a dohányzás.", "bOption": { "Temperature": 0.0, "Happyness": 3.0, "Wealthyness": 3.0, "CO": 2.0 } }, { "aText": "Nem alszom és elmegyek.", "bText": "Inkább alszom egy jót.", "minBudget": 11.0, "aOption": { "Temperature": 0.0, "Happyness": -1.0, "Wealthyness": 2.0, "CO": 2.0 }, "text": "Lehetőséged adódott, hogy tarts egy előadást, viszont ehhez rengeteget kéne autóznod, és korán kéne kelned.", "bOption": { "Temperature": 0.0, "Happyness": 0.0, "Wealthyness": 0.0, "CO": 0.0 } }, { "aText": "Elfogadom.", "bText": "Nem fogadom el.", "minBudget": 5.0, "aOption": { "Temperature": 0.0, "Happyness": -1.0, "Wealthyness": 1.0, "CO": 1.0 }, "text": "A főnököd előléptetést ajánlott fel. Ez viszont azt jelenti, hogy hosszabb lenne a munkaidőd.", "bOption": { "Temperature": 0.0, "Happyness": 0.0, "Wealthyness": 0.0, "CO": 0.0 } }, { "aText": "Jelenleg nincs rá igény.", "bText": "Fektessünk bele több pénzt!", "minBudget": 18.0, "aOption": { "Temperature": 0.0, "Happyness": 0.0, "Wealthyness": 0.0, "CO": 2.0 }, "text": "Az országgyűlés a megújjuló energia forrásokról szavaz.", "bOption": { "Temperature": 0.0, "Happyness": 1.0, "Wealthyness": 3.0, "CO": 0.0 } }, { "aText": "Elfogadom.", "bText": "Inkább buszozok.", "minBudget": 11.0, "aOption": { "Temperature": 0.0, "Happyness": 2.0, "Wealthyness": 0.0, "CO": 2.0 }, "text": "A főnököd felajánlott neked egy céges autót.", "bOption": { "Temperature": 0.0, "Happyness": -1.0, "Wealthyness": 1.0, "CO": 0.0 } }, { "aText": "Elmegyek venni.", "bText": "Úgy tűník ma mekizek.", "minBudget": 4.0, "aOption": { "Temperature": 0.0, "Happyness": 2.0, "Wealthyness": 3.0, "CO": 0.0 }, "text": "Elfogyott a saláta otthon.", "bOption": { "Temperature": 0.0, "Happyness": 3.0, "Wealthyness": 1.0, "CO": 2.0 } }, { "aText": "Ott leszek!", "bText": "Bocs, most nem jó.", "minBudget": 2.0, "aOption": { "Temperature": 0.0, "Happyness": 2.0, "Wealthyness": 0.0, "CO": 1.0 }, "text": "A barátod áthívott hamburgerezni. Átmész?", "bOption": { "Temperature": 0.0, "Happyness": -1.0, "Wealthyness": 0.0, "CO": 0.0 } }, { "aText": "Csak a szokásos mennyiséget.", "bText": "Egy kartonnal jöhet!", "minBudget": 4.0, "aOption": { "Temperature": 0.0, "Happyness": 0.0, "Wealthyness": 0.0, "CO": 1.0 }, "text": "Bevásárlóközpontban akciós a tej. Egy pár dobozzal több lehet jól fog jönni?", "bOption": { "Temperature": 0.0, "Happyness": -1.0, "Wealthyness": 1.0, "CO": 2.0 } }, { "aText": "Csak eldobom.", "bText": "Majd otthon kidobom.", "minBudget": 2.0, "aOption": { "Temperature": 0.0, "Happyness": -1.0, "Wealthyness": 0.0, "CO": 2.0 }, "text": "Nincs a közelben szemetes, pedig ki kéne dobnod egy zacskót.", "bOption": { "Temperature": 0.0, "Happyness": -1.0, "Wealthyness": 0.0, "CO": 0.0 } }, { "aText": "Utcán alszom.", "bText": "Taxit, legyen szíves!", "minBudget": 0.0, "aOption": { "Temperature": 0.0, "Happyness": -2.0, "Wealthyness": 0.0, "CO": 0.0 }, "text": "Barátaid elhívtak a közeli kocsmába és most haza kell jutnod valahogy.", "bOption": { "Temperature": 0.0, "Happyness": 1.0, "Wealthyness": 1.0, "CO": 2.0 } }, { "aText": "Itt az ideje lecserélni.", "bText": "Addig használom amíg jó.", "minBudget": 0.0, "aOption": { "Temperature": 0.0, "Happyness": 1.0, "Wealthyness": 2.0, "CO": 2.0 }, "text": "Kezd elöregedni a telefonod.", "bOption": { "Temperature": 0.0, "Happyness": -1.0, "Wealthyness": 0.0, "CO": 0.0 } }, { "aText": "", "bText": "", "minBudget": 0.0, "aOption": { "Temperature": 0.0, "Happyness": 0.0, "Wealthyness": 0.0, "CO": 0.0 }, "text": "", "bOption": { "Temperature": 0.0, "Happyness": 0.0, "Wealthyness": 0.0, "CO": 0.0 } } ]